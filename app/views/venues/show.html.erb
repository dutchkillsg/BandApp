<div id = "row">
	<div class=  "col venshow">
		<h1><%= @venue.name %></h1>
		<h2><%= @venue.city %></h2>
		<h2><%= @venue.state %></h2>
		<img src="<%= @venue.image %>">
		<h3>Family Friendly: <%= @venue.family_friendly %></h3>
	</div>
</div>
	<div class = 'col venevents'>
		<h1>Events:</h1>
		<% @event.each do |t| %>
		<li><h2><a href = '/events/<%= t.id %>show'><%= t.name %></a></h2></li>
		<% end %>
	</div>